{{!
    @template mod_booking/booked_users

    Classes required for JS:
    * none

    Data attributes required for JS:
    * data-region

     Example context (json):
    {
        "firstname" : "Anna",
        "lastname" : "X",
        "email" : "a.x@example.com",
        "status" : "x",
        "link" : "http://localhost:8010/user/profile.php?id=118"
    }
}}

 <li class="list-group-item list-group-item-action">
    <div>
        <a href="{{link}}">
            <h5 class="mb-1">
                {{firstname}} {{lastname}}
            </h5>
        </a>
    </div>
    <p class="mb-1">{{email}}</p>
</li>
